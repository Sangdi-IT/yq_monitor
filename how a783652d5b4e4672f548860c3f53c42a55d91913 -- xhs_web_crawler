[32m+[m[32m    async function loadYuqing() {[m
[32m+[m[32m      const res = await fetch('http://127.0.0.1:5000/review');[m
[32m+[m[32m      const data = await res.json();[m
[32m+[m[32m      const container = document.getElementById('yuqing-list');[m
[32m+[m[32m      container.innerHTML = '';[m
[32m+[m
[32m+[m[32m      data.forEach(item => {[m
[32m+[m[32m        const card = document.createElement('div');[m
[32m+[m[32m        card.className = 'card';[m
[32m+[m[32m        card.innerHTML = `[m
[32m+[m[32m          <p><strong>æ ‡é¢˜ï¼š</strong> ${item.title || 'ï¼ˆæ— æ ‡é¢˜ï¼‰'}</p>[m
[32m+[m[32m          <p><strong>å†…å®¹ï¼š</strong> ${item.content}</p>[m
[32m+[m[32m          <p><strong>æƒ…æ„Ÿï¼š</strong> ${item.sentiment} (${item.sentiment_score.toFixed(2)})</p>[m
[32m+[m[32m          <p><strong>é“¾æ¥ï¼š</strong> <a href="${item.url}" target="_blank">è·³è½¬æŸ¥çœ‹</a></p>[m
[32m+[m[32m          <button onclick="verify('${item.url}', true, this)">âœ… æ˜¯èˆ†æƒ…</button>[m
[32m+[m[32m          <button onclick="verify('${item.url}', false, this)">âŒ ä¸æ˜¯èˆ†æƒ…</button>[m
[32m+[m[32m        `;[m
[32m+[m[32m        container.appendChild(card);[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    async function verify(url, isYuqing, btn) {[m
[32m+[m[32m      const res = await fetch('http://127.0.0.1:5000/verify', {[m
[32m+[m[32m        method: 'POST',[m
[32m+[m[32m        headers: { 'Content-Type': 'application/json' },[m
[32m+[m[32m        body: JSON.stringify({ url, is_yuqing: isYuqing })[m
[32m+[m[32m      });[m
[32m+[m[32m      const result = await res.json();[m
[32m+[m[32m      if (result.success) {[m
[32m+[m[32m        if (!isYuqing) {[m
[32m+[m[32m          btn.parentElement.remove(); // åˆ é™¤å¡ç‰‡[m
[32m+[m[32m        } else {[m
[32m+[m[32m          btn.disabled = true;[m
[32m+[m[32m          btn.nextElementSibling.disabled = true;[m
[32m+[m[32m          btn.textContent = 'âœ… å·²ç¡®è®¤';[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        alert('æ“ä½œå¤±è´¥ï¼Œè¯·æ£€æŸ¥åç«¯æœåŠ¡');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    loadYuqing();[m
[32m+[m[32m  </script>[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
[1mdiff --git a/sentiment_analyzer.py b/sentiment_analyzer.py[m
[1mnew file mode 100644[m
[1mindex 0000000..51898a3[m
[1m--- /dev/null[m
[1m+++ b/sentiment_analyzer.py[m
[36m@@ -0,0 +1,155 @@[m
[32m+[m[32mimport json[m
[32m+[m[32mfrom datetime import datetime, timedelta[m
[32m+[m[32mimport re[m
[32m+[m[32mfrom snownlp import SnowNLP[m
[32m+[m[32mimport jieba[m
[32m+[m[32mimport argparse[m
[32m+[m[32mimport os[m
[32m+[m[32mfrom typing import Optional, Tuple, Dict, Any, List[m
[32m+[m
[32m+[m[32m# å®šä¹‰èˆ†æƒ…å…³é”®è¯åˆ—è¡¨ï¼ˆå¯è‡ªå®šä¹‰ï¼Œé’ˆå¯¹è´Ÿé¢æˆ–æ•æ„Ÿè¯é¢˜ï¼‰[m
[32m+[m[32mYUQING_KEYWORDS = ["æŠ•è¯‰", "å¤±æœ›", "é—®é¢˜", "æ›å…‰", "ç»´æƒ", "è´Ÿé¢", "å±æœº", "äº‹ä»¶", "ä¸æ»¡", "æ„¤æ€’"][m
[32m+[m
[32m+[m[32mdef clean_text(text: str) -> str:[m
[32m+[m[32m    """ç®€å•æ¸…æ´—æ–‡æœ¬ï¼šç§»é™¤ URLã€è¡¨æƒ…ã€å¤šä½™ç©ºæ ¼"""[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return ""[m
[32m+[m[32m    text = re.sub(r'http[s]?://\S+', '', text)  # ç§»é™¤ URL[m
[32m+[m[32m    text = re.sub(r'[^\u4e00-\u9fa5a-zA-Z0-9\s]', '', text)  # ç§»é™¤ç‰¹æ®Šå­—ç¬¦/è¡¨æƒ…[m
[32m+[m[32m    text = re.sub(r'\s+', ' ', text).strip()[m
[32m+[m[32m    return text[m
[32m+[m
[32m+[m[32mdef has_yuqing_keywords(text: str) -> bool:[m
[32m+[m[32m    """æ£€æŸ¥æ˜¯å¦åŒ…å«èˆ†æƒ…å…³é”®è¯ï¼šåˆ†è¯ + å­ä¸²åŒ¹é…ä»¥æé«˜å‘½ä¸­ç‡"""[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return False[m
[32m+[m[32m    try:[m
[32m+[m[32m        words = set(jieba.cut(text))[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        words = set(text.split())[m
[32m+[m[32m    # åˆ†è¯åŒ¹é…[m
[32m+[m[32m    if any(kw in words for kw in YUQING_KEYWORDS):[m
[32m+[m[32m        return True[m
[32m+[m[32m    # å­ä¸²åŒ¹é…ï¼ˆè¡¥å……ï¼‰[m
[32m+[m[32m    for kw in YUQING_KEYWORDS:[m
[32m+[m[32m        if kw in text:[m
[32m+[m[32m            return True[m
[32m+[m[32m    return False[m
[32m+[m
[32m+[m[32mdef analyze_sentiment(text: str, pos_thresh: float = 0.6, neg_thresh: float = 0.4) -> Tuple[str, float]:[m
[32m+[m[32m    """ä½¿ç”¨ SnowNLP è¿›è¡Œæƒ…æ„Ÿåˆ†æï¼Œå¼‚å¸¸æ—¶è¿”å› neutral"""[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return "neutral", 0.5[m
[32m+[m[32m    try:[m
[32m+[m[32m        s = SnowNLP(text)[m
[32m+[m[32m        score = float(s.sentiments)[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        return "neutral", 0.5[m
[32m+[m[32m    if score > pos_thresh:[m
[32m+[m[32m        return "positive", score[m
[32m+[m[32m    elif score < neg_thresh:[m
[32m+[m[32m        return "negative", score[m
[32m+[m[32m    else:[m
[32m+[m[32m        return "neutral", score[m
[32m+[m
[32m+[m[32mdef parse_post_time(post: Dict[str, Any]) -> Optional[datetime]:[m
[32m+[m[32m    """å°è¯•ä» post ä¸­è§£ææ—¶é—´ï¼Œæ”¯æŒ readable_time å’Œæ•°å€¼ timeï¼ˆæ¯«ç§’/ç§’ï¼‰"""[m
[32m+[m[32m    rt = post.get("readable_time") or post.get("readableTime") or post.get("readable")[m
[32m+[m[32m    if rt and isinstance(rt, str):[m
[32m+[m[32m        for fmt in ("%Y-%m-%d %H:%M:%S", "%Y/%m/%d %H:%M:%S"):[m
[32m+[m[32m            try:[m
[32m+[m[32m                return datetime.strptime(rt, fmt)[m
[32m+[m[32m            except Exception:[m
[32m+[m[32m                continue[m
[32m+[m[32m    t = post.get("time")[m
[32m+[m[32m    if isinstance(t, (int, float)):[m
[32m+[m[32m        # åˆ¤æ–­æ˜¯æ¯«ç§’è¿˜æ˜¯ç§’[m
[32m+[m[32m        if t > 1e12:[m
[32m+[m[32m            return datetime.fromtimestamp(t / 1000.0)[m
[32m+[m[32m        elif t > 1e9:[m
[32m+[m[32m            return datetime.fromtimestamp(t)[m
[32m+[m[32m    return None[m
[32m+[m
[32m+[m[32mdef is_recent(post_time: Optional[datetime], days: int = 1) -> bool:[m
[32m+[m[32m    """æ£€æŸ¥å‘å¸ƒæ—¶é—´æ˜¯å¦åœ¨æœ€è¿‘ days å¤©å†…"""[m
[32m+[m[32m    if not post_time:[m
[32m+[m[32m        return False[m
[32m+[m[32m    try:[m
[32m+[m[32m        return post_time >= (datetime.now() - timedelta(days=days))[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        return False[m
[32m+[m
[32m+[m[32mdef process_posts(input_file: str,[m
[32m+[m[32m                  output_file: str,[m
[32m+[m[32m                  days: int = 1,[m
[32m+[m[32m                  pos_thresh: float = 0.6,[m
[32m+[m[32m                  neg_thresh: float = 0.4) -> None:[m
[32m+[m[32m    """ä¸»å¤„ç†å‡½æ•°ï¼šè¯»å– JSONï¼Œåˆ†æå¹¶è¾“å‡ºèˆ†æƒ…å¸–å­"""[m
[32m+[m[32m    if not os.path.exists(input_file):[m
[32m+[m[32m        print(f"è¾“å…¥æ–‡ä»¶ä¸å­˜åœ¨: {input_file}")[m
[32m+[m[32m        return[m
[32m+[m
[32m+[m[32m    try:[m
[32m+[m[32m        with open(input_file, 'r', encoding='utf-8') as f:[m
[32m+[m[32m            posts = json.load(f)[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        print(f"è¯»å– JSON å¤±è´¥: {e}")[m
[32m+[m[32m        return[m
[32m+[m
[32m+[m[32m    yuqing_posts: List[Dict[str, Any]] = [][m
[32m+[m[32m    stats = {"total": 0, "yuqing_count": 0, "negative": 0, "neutral": 0, "positive": 0}[m
[32m+[m
[32m+[m[32m    for post in posts:[m
[32m+[m[32m        title = post.get("unified_title", "") or post.get("title", "")[m
[32m+[m[32m        desc = post.get("desc", "") or post.get("content", "")[m
[32m+[m[32m        full_text = clean_text(f"{title} {desc}").strip()[m
[32m+[m[32m        if not full_text:[m
[32m+[m[32m            continue[m
[32m+[m
[32m+[m[32m        sentiment, score = analyze_sentiment(full_text, pos_thresh=pos_thresh, neg_thresh=neg_thresh)[m
[32m+[m[32m        has_keywords = has_yuqing_keywords(full_text)[m
[32m+[m[32m        post_time = parse_post_time(post)[m
[32m+[m[32m        recent = is_recent(post_time, days=days)[m
[32m+[m
[32m+[m[32m        is_yuqing = (sentiment == "negative" or has_keywords) and recent[m
[32m+[m
[32m+[m[32m        post["cleaned_text"] = full_text[m
[32m+[m[32m        post["sentiment"] = sentiment[m
[32m+[m[32m        post["sentiment_score"] = score[m
[32m+[m[32m        post["has_yuqing_keywords"] = has_keywords[m
[32m+[m[32m        post["is_yuqing"] = is_yuqing[m
[32m+[m[32m        if post_time:[m
[32m+[m[32m            post["parsed_time"] = post_time.strftime("%Y-%m-%d %H:%M:%S")[m
[32m+[m
[32m+[m[32m        stats["total"] += 1[m
[32m+[m[32m        stats[sentiment] = stats.get(sentiment, 0) + 1[m
[32m+[m[32m        if is_yuqing:[m
[32m+[m[32m            stats["yuqing_count"] += 1[m
[32m+[m[32m            yuqing_posts.append(post)[m
[32m+[m
[32m+[m[32m    try:[m
[32m+[m[32m        with open(output_file, 'w', encoding='utf-8') as f:[m
[32m+[m[32m            json.dump(yuqing_posts, f, ensure_ascii=False, indent=4)[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        print(f"å†™å…¥è¾“å‡ºæ–‡ä»¶å¤±è´¥: {e}")[m
[32m+[m[32m        return[m
[32m+[m
[32m+[m[32m    total = stats["total"] or 1[m
[32m+[m[32m    print(f"å¤„ç†å®Œæˆï¼æ€»å¸–å­: {stats['total']}")[m
[32m+[m[32m    print(f"èˆ†æƒ…å¸–å­: {stats['yuqing_count']} ({stats['yuqing_count']/total*100:.2f}%)")[m
[32m+[m[32m    print(f"æƒ…æ„Ÿåˆ†å¸ƒ: æ­£é¢ {stats['positive']}, ä¸­æ€§ {stats['neutral']}, è´Ÿé¢ {stats['negative']}")[m
[32m+[m[32m    print(f"è¾“å‡ºæ–‡ä»¶: {output_file}")[m
[32m+[m
[32m+[m[32mdef main():[m
[32m+[m[32m    parser = argparse.ArgumentParser(description="èˆ†æƒ…æƒ…æ„Ÿåˆ†æè„šæœ¬")[m
[32m+[m[32m    parser.add_argument("-i", "--input", required=False, default="yq_monitor.json", help="è¾“å…¥ JSON æ–‡ä»¶è·¯å¾„ï¼ˆé»˜è®¤ redbook.jsonï¼‰")[m
[32m+[m[32m    parser.add_argument("-o", "--output", required=False, default="yuqing_posts.json", help="è¾“å‡ºèˆ†æƒ… JSON æ–‡ä»¶è·¯å¾„ï¼ˆé»˜è®¤ yuqing_posts.jsonï¼‰")[m
[32m+[m[32m    parser.add_argument("-d", "--days", type=int, default=1, help="åˆ¤æ–­ä¸ºè¿‘æœŸçš„å¤©æ•°ï¼ˆé»˜è®¤1å¤©ï¼‰")[m
[32m+[m[32m    parser.add_argument("--pos", type=float, default=0.6, help="æ­£é¢é˜ˆå€¼ï¼ˆé»˜è®¤0.6ï¼‰")[m
[32m+[m[32m    parser.add_argument("--neg", type=float, default=0.4, help="è´Ÿé¢é˜ˆå€¼ï¼ˆé»˜è®¤0.4ï¼‰")[m
[32m+[m[32m    args = parser.parse_args()[m
[32m+[m
[32m+[m[32m    process_posts(args.input, args.output, days=args.days, pos_thresh=args.pos, neg_thresh=args.neg)[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    main()[m
