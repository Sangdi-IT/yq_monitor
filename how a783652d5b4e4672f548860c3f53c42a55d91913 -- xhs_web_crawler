[32m+[m[32mimport json[m
[32m+[m[32mimport base64[m
[32m+[m[32mfrom typing import Dict, List, Union, Any, Optional[m
[32m+[m[32mimport os[m
[32m+[m[32mimport sys[m
[32m+[m[32mfrom datetime import datetime[m
[32m+[m[32mimport zoneinfo  # For timezone handling (Python 3.9+)[m
[32m+[m
[32m+[m[32mclass HarExtractor:[m
[32m+[m[32m    def __init__(self, har_file: str):[m
[32m+[m[32m        self.har_file = har_file[m
[32m+[m[32m        self.content_list = [][m
[32m+[m[32m        print(f"\nåˆå§‹åŒ– HAR æå–å™¨... ç›®æ ‡æ–‡ä»¶: {har_file}")[m
[32m+[m
[32m+[m[32m    def decode_base64_content(self, content: str) -> str:[m
[32m+[m[32m        try:[m
[32m+[m[32m            padding = 4 - (len(content) % 4)[m
[32m+[m[32m            if padding != 4:[m
[32m+[m[32m                content += '=' * padding[m
[32m+[m[32m            return base64.b64decode(content).decode('utf-8')[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(f"Base64 è§£ç å¤±è´¥: {str(e)}")[m
[32m+[m[32m            return content[m
[32m+[m
[32m+[m[32m    def extract_note_cards(self, content: Any) -> List[Dict]:[m
[32m+[m[32m        note_cards = [][m
[32m+[m[32m        try:[m
[32m+[m[32m            if isinstance(content, dict):[m
[32m+[m[32m                items = content.get('data', {}).get('items', [])[m
[32m+[m[32m                note_cards = [item['note_card'] for item in items if item.get('model_type') == 'note' and 'note_card' in item][m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(f"æå– note_card æ—¶å‘ç”Ÿé”™è¯¯: {str(e)}")[m
[32m+[m[32m        return note_cards[m
[32m+[m
[32m+[m[32m    def process_content(self, content_text: str, url: str) -> List[Dict]:[m
[32m+[m[32m        try:[m
[32m+[m[32m            parsed_content = json.loads(content_text)[m
[32m+[m[32m        except json.JSONDecodeError:[m
[32m+[m[32m            try:[m
[32m+[m[32m                decoded_content = self.decode_base64_content(content_text)[m
[32m+[m[32m                parsed_content = json.loads(decoded_content)[m
[32m+[m[32m            except (json.JSONDecodeError, UnicodeDecodeError):[m
[32m+[m[32m                print(f"è­¦å‘Š: URL {url} çš„å†…å®¹æ—¢ä¸æ˜¯æœ‰æ•ˆçš„ JSON ä¹Ÿä¸æ˜¯æœ‰æ•ˆçš„ base64ï¼Œå°†è·³è¿‡")[m
[32m+[m[32m                return [][m
[32m+[m[32m        return self.extract_note_cards(parsed_content)[m
[32m+[m
[32m+[m[32m    def unify_titles(self) -> None:[m
[32m+[m[32m        for note in self.content_list:[m
[32m+[m[32m            note['unified_title'] = note.get('display_title') or note.get('title') or ''[m
[32m+[m
[32m+[m[32m    def convert_timestamps(self) -> None:[m
[32m+[m[32m        tz = zoneinfo.ZoneInfo("Asia/Shanghai")  # Beijing time (UTC+8)[m
[32m+[m[32m        for note in self.content_list:[m
[32m+[m[32m            for field, readable_field in [('time', 'readable_time'), ('current_time', 'readable_current_time')]:[m
[32m+[m[32m                if field in note:[m
[32m+[m[32m                    try:[m
[32m+[m[32m                        timestamp_ms = int(note[field])[m
[32m+[m[32m                        timestamp_s = timestamp_ms / 1000[m
[32m+[m[32m                        dt_utc = datetime.fromtimestamp(timestamp_s, tz=zoneinfo.ZoneInfo("UTC"))[m
[32m+[m[32m                        dt_local = dt_utc.astimezone(tz)[m
[32m+[m[32m                        note[readable_field] = dt_local.strftime('%Y-%m-%d %H:%M:%S')[m
[32m+[m[32m                    except (ValueError, TypeError):[m
[32m+[m[32m                        print(f"è­¦å‘Š: æ— æ³•è½¬æ¢ '{field}' å­—æ®µ: {note.get(field)}")[m
[32m+[m
[32m+[m[32m    def extract_content(self) -> List[Dict[str, Any]]:[m
[32m+[m[32m        print("\nå¼€å§‹æå–å†…å®¹...")[m
[32m+[m[32m        har_data = self.read_har_file()[m
[32m+[m[32m        entries = har_data.get('log', {}).get('entries', [])[m
[32m+[m[32m        print(f"æ‰¾åˆ° {len(entries)} ä¸ªè¯·æ±‚è®°å½•")[m
[32m+[m[41m        [m
[32m+[m[32m        for entry in entries:[m
[32m+[m[32m            # Unified handling for both formats[m
[32m+[m[32m            content = entry.get('content') or entry.get('response', {}).get('content', {})[m
[32m+[m[32m            url = entry.get('url') or entry.get('request', {}).get('url', '')[m
[32m+[m[32m            if content and 'text' in content:[m
[32m+[m[32m                note_cards = self.process_content(content['text'], url)[m
[32m+[m[32m                if note_cards:[m
[32m+[m[32m                    self.content_list.extend(note_cards)[m
[32m+[m[32m                    print(f"æˆåŠŸæå– URL: {url} çš„ {len(note_cards)} ä¸ªç¬”è®°")[m
[32m+[m[41m        [m
[32m+[m[32m        self.unify_titles()[m
[32m+[m[32m        self.convert_timestamps()[m
[32m+[m[32m        print(f"\nå†…å®¹æå–å®Œæˆï¼å…±æå–äº† {len(self.content_list)} ä¸ªå†…å®¹é¡¹")[m
[32m+[m[32m        return self.content_list[m
[32m+[m
[32m+[m[32m    def read_har_file(self) -> Dict:[m
[32m+[m[32m        print("æ­£åœ¨è¯»å– HAR æ–‡ä»¶...")[m
[32m+[m[32m        try:[m
[32m+[m[32m            with open(self.har_file, 'r', encoding='utf-8') as f:[m
[32m+[m[32m                return json.load(f)[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            raise Exception(f"è¯»å– HAR æ–‡ä»¶å¤±è´¥: {str(e)}")[m
[32m+[m
[32m+[m[32m    def save_to_json(self, output_file: str = None) -> str:[m
[32m+[m[32m        if not self.content_list:[m
[32m+[m[32m            raise Exception("æ²¡æœ‰è¦ä¿å­˜çš„å†…å®¹ï¼Œè¯·å…ˆè°ƒç”¨ extract_content()")[m
[32m+[m[41m        [m
[32m+[m[32m        if output_file is None:[m
[32m+[m[32m            base_name = os.path.splitext(os.path.basename(self.har_file))[0][m
[32m+[m[32m            output_file = f"{base_name}_content.json"[m
[32m+[m[41m        [m
[32m+[m[32m        print(f"\næ­£åœ¨ä¿å­˜å†…å®¹åˆ°æ–‡ä»¶: {output_file}")[m
[32m+[m[32m        try:[m
[32m+[m[32m            with open(output_file, 'w', encoding='utf-8') as f:[m
[32m+[m[32m                json.dump(self.content_list, f, ensure_ascii=False, indent=2)[m
[32m+[m[32m            print("æ–‡ä»¶ä¿å­˜æˆåŠŸï¼")[m
[32m+[m[32m            return output_file[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            raise Exception(f"ä¿å­˜ JSON æ–‡ä»¶å¤±è´¥: {str(e)}")[m
[32m+[m
[32m+[m[32mdef process_har_file(har_file: str, output_file: str = None) -> Dict[str, Any]:[m
[32m+[m[32m    start_time = datetime.now()[m
[32m+[m[32m    try:[m
[32m+[m[32m        extractor = HarExtractor(har_file)[m
[32m+[m[32m        content_list = extractor.extract_content()[m
[32m+[m[32m        output_path = extractor.save_to_json(output_file)[m
[32m+[m[32m        processing_time = (datetime.now() - start_time).total_seconds()[m
[32m+[m[32m        return {[m
[32m+[m[32m            'success': True,[m
[32m+[m[32m            'message': f'æˆåŠŸæå– {len(content_list)} ä¸ªå†…å®¹é¡¹',[m
[32m+[m[32m            'output_file': output_path,[m
[32m+[m[32m            'content_count': len(content_list),[m
[32m+[m[32m            'processing_time': processing_time[m
[32m+[m[32m        }[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        return {[m
[32m+[m[32m            'success': False,[m
[32m+[m[32m            'message': str(e),[m
[32m+[m[32m            'output_file': None,[m
[32m+[m[32m            'content_count': 0[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32mdef main():[m
[32m+[m[32m    print("\n=== HAR æ–‡ä»¶å†…å®¹æå–å·¥å…· ===")[m
[32m+[m[41m    [m
[32m+[m[32m    while True:[m
[32m+[m[32m        har_path = input("\nè¯·è¾“å…¥HARæ–‡ä»¶è·¯å¾„: ").strip()[m
[32m+[m[32m        if har_path and os.path.exists(har_path) and har_path.lower().endswith('.har'):[m
[32m+[m[32m            break[m
[32m+[m[32m        print("é”™è¯¯ï¼šæ— æ•ˆçš„ HAR æ–‡ä»¶è·¯å¾„")[m
[32m+[m
[32m+[m[32m    output_path = input("\nè¯·è¾“å…¥è¾“å‡ºæ–‡ä»¶è·¯å¾„ï¼ˆç›´æ¥å›è½¦ä½¿ç”¨é»˜è®¤è·¯å¾„ï¼‰: ").strip() or None[m
[32m+[m[32m    if output_path and not output_path.lower().endswith('.json'):[m
[32m+[m[32m        output_path += '.json'[m
[32m+[m
[32m+[m[32m    print("\nå¼€å§‹å¤„ç†...")[m
[32m+[m[32m    result = process_har_file(har_path, output_path)[m
[32m+[m[41m    [m
[32m+[m[32m    print("\n=== å¤„ç†ç»“æœ ===")[m
[32m+[m[32m    if result['success']:[m
[32m+[m[32m        print(f"çŠ¶æ€: æˆåŠŸ\nè¾“å‡ºæ–‡ä»¶: {result['output_file']}\næå–å†…å®¹æ•°: {result['content_count']}\nå¤„ç†æ—¶é—´: {result['processing_time']:.2f} ç§’")[m
[32m+[m[32m    else:[m
[32m+[m[32m        print(f"çŠ¶æ€: å¤±è´¥\né”™è¯¯ä¿¡æ¯: {result['message']}")[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    try:[m
[32m+[m[32m        main()[m
[32m+[m[32m    except KeyboardInterrupt:[m
[32m+[m[32m        print("\n\nç¨‹åºå·²è¢«ç”¨æˆ·ä¸­æ–­")[m
[32m+[m[32m        sys.exit(0)[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        print(f"\nç¨‹åºå‘ç”Ÿé”™è¯¯: {str(e)}")[m
[32m+[m[32m        sys.exit(1)[m
[32m+[m[32m    finally:[m
[32m+[m[32m        print("\nç¨‹åºç»“æŸ")[m
\ No newline at end of file[m
