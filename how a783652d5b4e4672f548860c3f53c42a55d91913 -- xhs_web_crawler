[32m+[m[32mfrom flask import Flask, request, jsonify[m
[32m+[m[32mfrom datetime import datetime, timedelta[m
[32m+[m[32mimport json[m
[32m+[m[32mimport re[m
[32m+[m[32mfrom snownlp import SnowNLP[m
[32m+[m[32mimport jieba[m
[32m+[m[32mfrom flask_cors import CORS[m
[32m+[m[32mapp = Flask(__name__)[m
[32m+[m[32mCORS(app)[m
[32m+[m
[32m+[m
[32m+[m[32mYUQING_KEYWORDS = ["æŠ•è¯‰", "å¤±æœ›", "é—®é¢˜", "æ›å…‰", "ç»´æƒ", "è´Ÿé¢", "å±æœº", "äº‹ä»¶", "ä¸æ»¡", "æ„¤æ€’"][m
[32m+[m
[32m+[m[32mdef clean_text(text: str) -> str:[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return ""[m
[32m+[m[32m    text = re.sub(r'http[s]?://\S+', '', text)[m
[32m+[m[32m    text = re.sub(r'[^\u4e00-\u9fa5a-zA-Z0-9\s]', '', text)[m
[32m+[m[32m    text = re.sub(r'\s+', ' ', text).strip()[m
[32m+[m[32m    return text[m
[32m+[m
[32m+[m[32mdef has_yuqing_keywords(text: str) -> bool:[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return False[m
[32m+[m[32m    try:[m
[32m+[m[32m        words = set(jieba.cut(text))[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        words = set(text.split())[m
[32m+[m[32m    if any(kw in words for kw in YUQING_KEYWORDS):[m
[32m+[m[32m        return True[m
[32m+[m[32m    for kw in YUQING_KEYWORDS:[m
[32m+[m[32m        if kw in text:[m
[32m+[m[32m            return True[m
[32m+[m[32m    return False[m
[32m+[m
[32m+[m[32mdef analyze_sentiment(text: str, pos_thresh: float = 0.6, neg_thresh: float = 0.4):[m
[32m+[m[32m    if not text:[m
[32m+[m[32m        return "neutral", 0.5[m
[32m+[m[32m    try:[m
[32m+[m[32m        s = SnowNLP(text)[m
[32m+[m[32m        score = float(s.sentiments)[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        return "neutral", 0.5[m
[32m+[m[32m    if score > pos_thresh:[m
[32m+[m[32m        return "positive", score[m
[32m+[m[32m    elif score < neg_thresh:[m
[32m+[m[32m        return "negative", score[m
[32m+[m[32m    else:[m
[32m+[m[32m        return "neutral", score[m
[32m+[m
[32m+[m[32mdef parse_post_time(post):[m
[32m+[m[32m    rt = post.get("readable_time") or post.get("readableTime") or post.get("readable")[m
[32m+[m[32m    if rt and isinstance(rt, str):[m
[32m+[m[32m        for fmt in ("%Y-%m-%d %H:%M:%S", "%Y/%m/%d %H:%M:%S"):[m
[32m+[m[32m            try:[m
[32m+[m[32m                return datetime.strptime(rt, fmt)[m
[32m+[m[32m            except Exception:[m
[32m+[m[32m                continue[m
[32m+[m[32m    t = post.get("time")[m
[32m+[m[32m    if isinstance(t, (int, float)):[m
[32m+[m[32m        if t > 1e12:[m
[32m+[m[32m            return datetime.fromtimestamp(t / 1000.0)[m
[32m+[m[32m        elif t > 1e9:[m
[32m+[m[32m            return datetime.fromtimestamp(t)[m
[32m+[m[32m    return None[m
[32m+[m
[32m+[m[32mdef is_recent(post_time, days: int = 100):[m
[32m+[m[32m    if not post_time:[m
[32m+[m[32m        return False[m
[32m+[m[32m    try:[m
[32m+[m[32m        return post_time >= (datetime.now() - timedelta(days=days))[m
[32m+[m[32m    except Exception:[m
[32m+[m[32m        return False[m
[32m+[m[41m    [m
[32m+[m[32m@app.route('/analyze', methods=['POST'])[m
[32m+[m[32mdef analyze_post():[m
[32m+[m[32m    raw = request.get_json(force=True)[m
[32m+[m[32m    post = raw.get("post", {})[m
[32m+[m[32m    url = raw.get("url", "")[m
[32m+[m[32m    items = post.get("items", [])[m
[32m+[m[32m    if not items:[m
[32m+[m[32m        return jsonify({"error": "No items found"}), 400[m
[32m+[m
[32m+[m[32m    item = items[0][m
[32m+[m[32m    note_card = item.get("note_card", {})[m
[32m+[m[32m    title = note_card.get("title", "")[m
[32m+[m[32m    content = note_card.get("desc", "")[m
[32m+[m[32m    timestamp = note_card.get("time")[m
[32m+[m[32m    location = note_card.get("ip_location", "")[m
[32m+[m[32m    user_info = note_card.get("user", {})[m
[32m+[m[32m    nickname = user_info.get("nickname", "")[m
[32m+[m[32m    user_id = user_info.get("user_id", "")[m
[32m+[m[32m    interact = note_card.get("interact_info", {})[m
[32m+[m[32m    liked_count = interact.get("liked_count", "0")[m
[32m+[m[32m    comment_count = interact.get("comment_count", "0")[m
[32m+[m[32m    share_count = interact.get("share_count", "0")[m
[32m+[m[32m    tags = note_card.get("tag_list", [])[m
[32m+[m
[32m+[m[32m    full_text = clean_text(f"{title} {content}")[m
[32m+[m[32m    post_time = parse_post_time({"time": timestamp})[m
[32m+[m[32m    sentiment, score = analyze_sentiment(full_text)[m
[32m+[m[32m    has_keywords = has_yuqing_keywords(full_text)[m
[32m+[m[32m    recent = is_recent(post_time)[m
[32m+[m[32m    is_yuqing = (sentiment == "negative" or has_keywords) and recent[m
[32m+[m[41m    [m
[32m+[m[32m    # æ„å»ºç»“æœå­—å…¸[m
[32m+[m[32m    result = {[m
[32m+[m[32m        "is_yuqing": is_yuqing,[m
[32m+[m[32m        "sentiment": sentiment,[m
[32m+[m[32m        "sentiment_score": score,[m
[32m+[m[32m        "has_yuqing_keywords": has_keywords,[m
[32m+[m[32m        "cleaned_text": full_text,[m
[32m+[m[32m        "parsed_time": post_time.strftime("%Y-%m-%d %H:%M:%S") if post_time else None,[m
[32m+[m[32m        "title": title,[m
[32m+[m[32m        "content": content,[m
[32m+[m[32m        "timestamp": timestamp,[m
[32m+[m[32m        "location": location,[m
[32m+[m[32m        "user_nickname": nickname,[m
[32m+[m[32m        "user_id": user_id,[m
[32m+[m[32m        "liked_count": liked_count,[m
[32m+[m[32m        "comment_count": comment_count,[m
[32m+[m[32m        "share_count": share_count,[m
[32m+[m[32m        "tags": [tag.get("name") for tag in tags],[m
[32m+[m[32m        "url": url,[m
[32m+[m[32m        "human_verified": None  # None è¡¨ç¤ºæœªå®¡æ ¸ï¼Œtrue/false è¡¨ç¤ºäººå·¥åˆ¤æ–­ç»“æœ[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if is_yuqing:[m
[32m+[m[32m        with open("yuqing_log.json", "a", encoding="utf-8") as f:[m
[32m+[m[32m            f.write(json.dumps(result, ensure_ascii=False) + "\n")[m
[32m+[m
[32m+[m[32m    return jsonify(result)[m
[32m+[m[32m@app.route('/review', methods=['GET'])[m
[32m+[m[32mdef review_yuqing():[m
[32m+[m[32m    results = [][m
[32m+[m[32m    try:[m
[32m+[m[32m        with open("yuqing_log.json", "r", encoding="utf-8") as f:[m
[32m+[m[32m            for line in f:[m
[32m+[m[32m                try:[m
[32m+[m[32m                    item = json.loads(line.strip())[m
[32m+[m[32m                    if item.get("human_verified") is not False:  # åªå±•ç¤ºæœªå®¡æ ¸æˆ–ç¡®è®¤æ˜¯èˆ†æƒ…çš„[m
[32m+[m[32m                        results.append(item)[m
[32m+[m[32m                except Exception:[m
[32m+[m[32m                    continue[m
[32m+[m[32m    except FileNotFoundError:[m
[32m+[m[32m        results = [][m
[32m+[m[32m    return jsonify(results)[m
[32m+[m[32m@app.route('/verify', methods=['POST'])[m
[32m+[m[32mdef verify_yuqing():[m
[32m+[m[32m    data = request.get_json(force=True)[m
[32m+[m[32m    url = data.get("url")[m
[32m+[m[32m    is_yuqing = data.get("is_yuqing")  # äººå·¥åˆ¤æ–­ç»“æœï¼šTrue æˆ– False[m
[32m+[m
[32m+[m[32m    updated = [][m
[32m+[m[32m    try:[m
[32m+[m[32m        with open("yuqing_log.json", "r", encoding="utf-8") as f:[m
[32m+[m[32m            for line in f:[m
[32m+[m[32m                item = json.loads(line.strip())[m
[32m+[m[32m                if item.get("url") == url:[m
[32m+[m[32m                    item["human_verified"] = is_yuqing[m
[32m+[m[32m                updated.append(item)[m
[32m+[m[32m        with open("yuqing_log.json", "w", encoding="utf-8") as f:[m
[32m+[m[32m            for item in updated:[m
[32m+[m[32m                f.write(json.dumps(item, ensure_ascii=False) + "\n")[m
[32m+[m[32m        return jsonify({"success": True})[m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        return jsonify({"success": False, "error": str(e)}), 500[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    app.run(host='127.0.0.1', port=5000)[m
